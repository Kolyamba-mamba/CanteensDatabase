<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/routes"
    app:startDestination="@id/listOfOrdersFragment">
    <fragment
        android:id="@+id/listOfOrdersFragment"
        android:name="com.example.canteen.ui.Worker.ListOfOrdersFragment"
        android:label="@string/listOfOrders"
        tools:layout="@layout/fragment_list_of_orders" >
        <action
            android:id="@+id/action_listOfOrdersFragment_to_orderInformationFragment"
            app:destination="@id/orderInformationFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderInformationFragment"
        android:name="com.example.canteen.ui.Worker.OrderInformationFragment"
        android:label="@string/OrderInformation"
        tools:layout="@layout/fragment_order_information" />
</navigation>